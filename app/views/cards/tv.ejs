<div class="card-image waves-effect waves-block waves-light">
    <img class="activator" src=<%= "https://image.tmdb.org/t/p/w500" + file.poster_path %>>
</div>
<div class="card-content">
    <span class="card-title activator grey-text text-darken-4"><%= file.name %> %><i
            class="material-icons right">more_vert</i></span>
    <p><%= file.overview %></p>
</div>
<div class="card-reveal">
    <span class="card-title grey-text text-darken-4"><%= file.name %> %><i class="material-icons right">close</i></span>
    <p><strong>Popularidade:</strong> <%= file.popularity %></p>
    <p><strong>Avaliação:</strong> <%= file.vote_average %></p>
    <p><strong>Data do Primeiro Episódio:</strong>
        <%= new Date(file.first_air_date).toLocaleDateString() %></p>
    <p><strong>País de Origem:</strong> <%= file.origin_country %></p>
    <p><strong>Idioma Original:</strong> <%= file.original_language %></p>
    <p><strong>Título Original:</strong> <%= file.original_name %></p>
    <!-- <p><%= file.poster_path %></p> -->
    <!-- <p><%= file.id %></p> -->
    <!-- <p><%= file.backdrop_path %></p> -->
    <!-- <p><%= file.overview %></p> -->
    <!-- <p><%= file.genre_ids %></p> -->
    <!-- <p><%= file.vote_count %></p> -->
    <!-- <p><%= file.name %></p> -->
    <a class="waves-effect waves-light btn" href=<%= "http://localhost:3000/media/stream/" + file.id %>>Exibir</a>
</div>